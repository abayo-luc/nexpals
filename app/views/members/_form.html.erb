<%= form_for @member, :html => {:multipart => true} do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <br>
  <%= f.label :short_bio %>
  <%= f.text_field :short_bio %>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <label class="btn btn-primary">
  <%= f.label :photo %>
    <span style="display:none;">
      <%= f.file_field :photo %>
    </span>
  </label>
  <br>
  <%= f.submit %>
<% end %>