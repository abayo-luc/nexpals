<div class="row">
  
  <!-- Carousel -->
  <div id="carousel-example-generic" class="carousel slide" data-ride="carousel" >
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1"></li>
      <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox" style="width: 100%; height: 350px">
      <div class="item active">
        <img src="#" alt="...">
      </div>

      <div class="item">
        <img src="#" class ="img-responsive" alt="...">
        <div class="carousel-caption">

        </div>

      </div>

      <!-- Controls -->
      <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>



<!-- Team NexPals -->
<div class="col-md-5" style="margin: 20px;">
  
  <h2 class="text-center main-color"><b>NexPals Team</b></h2>
  <hr>
  
  <div class="text-center">
    <a href="/members/new" data-toggle="tooltip" data-placement="top" title="add"> <i class="glyphicon glyphicon-plus" style="font-size: 30px;"></i></a>
  </div>


 <!-- first team member -->
 <div class="nexer">
    
      <div class="col-xs-12 col-sm-3">

        <img src="http://img.timeinc.net/time/daily/2010/1011/poy_nomination_agassi.jpg" alt="image" class="img-responsive img-circle" />

        <div class="text-center">
          <button href="" class="w3-button w3-round-xlarge edit-button">Edit</button>
          <button href="" class="w3-button w3-round-xlarge delete-button">Fire</button>
        </div>
      </div>

      <div class="col-xs-12 col-sm-9 text-center">
       
        <span class="name"> <b>Name</b> | Title </span> <br>
        
        <span class="glyphicon glyphicon-map-marker text-muted c-info" data-toggle="tooltip" title="address"></span>
        <span class="visible-xs"> <span class="text-muted">street address</span><br/></span>
        <span class="glyphicon glyphicon-earphone text-muted c-info" data-toggle="tooltip" title="phone"></span>
        <span class="visible-xs"> <span class="text-muted"> helpline </span><br/></span>

        <span class="glyphicon glyphicon-envelope text-muted c-info" data-toggle="tooltip" title="email"></span>
        <span class="visible-xs"> <span class="text-muted">email</span><br/></span>

       
         <br>
         <br> 
        <p> Short Bio</p> <br>

      </div>
      <div class="clearfix"></div>
   
    </div>  
</div>


<!-- partners -->

<div class="col-md-3" style="margin: 20px; margin-left: 30px;">
  <h2 class="text-center main-color"><b>Partners</b></h2>
  <hr>

  <div class="text-center">
    <a href="/members/new" data-toggle="tooltip" data-placement="top" title="add"> <i class="glyphicon glyphicon-plus" style="font-size: 30px; text-align: center;"></i></a>
  </div>

  <!-- first partner -->
    
   <div class="nex-partner-box">
    
      <div class="col-xs-12 col-sm-3" >
        <div style="width: 90px; height: auto;">
          <img src="http://img.timeinc.net/time/daily/2010/1011/poy_nomination_agassi.jpg" alt="image" class="img-responsive img-circle"/>
        </div>

      </div>

      <div class="col-xs-12 col-sm-9 text-center">
       
        <span class="name"> <b> Company Name </b> </span> <br>
        
        <span class="glyphicon glyphicon-map-marker text-muted c-info" data-toggle="tooltip" title="address"></span>
        <span class="visible-xs"> <span class="text-muted">street address</span><br/></span>
        <span class="glyphicon glyphicon-earphone text-muted c-info" data-toggle="tooltip" title="phone"></span>
        <span class="visible-xs"> <span class="text-muted"> helpline </span><br/></span>

        <span class="fa fa-comments text-muted c-info" data-toggle="tooltip" title="email"></span>
        <span class="visible-xs"> <span class="text-muted">email</span><br/></span>

        <span class="glyphicon glyphicon-heart text-muted c-info" data-toggle="tooltip" title="other"> </span>
       
         <br>
         <br> 
        <p> Short Description</p> <br>
    
        
        <div class="text-center">
          <button href="" class="w3-button w3-round-xlarge edit-button">Edit</button>
          <button href="" class="w3-button w3-round-xlarge delete-button">Remove</button>
        </div>
        

      </div>
      <div class="clearfix"></div>
   
    </div>

    <hr>

    
  
</div>


  <!-- media feeds -->
  <%= render "pages/media" %>

</div>